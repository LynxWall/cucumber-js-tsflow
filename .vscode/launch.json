{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug All",
			"type": "pwa-node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/@lynxwall/cucumber-tsflow/bin/cucumber-tsflow",
			"stopOnEntry": true,
			"args": ["-p", "default"],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": ["--nolazy"],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"console": "integratedTerminal",
			"sourceMaps": true,
			"outDir": null
		},
		{
			"name": "Debug Feature",
			"type": "pwa-node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/@lynxwall/cucumber-tsflow/bin/cucumber-tsflow",
			"stopOnEntry": true,
			"args": ["--debug-file", "${file}", "-p", "default"],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": ["--nolazy"],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"console": "integratedTerminal",
			"sourceMaps": true,
			"outDir": null
		}
	]
}

