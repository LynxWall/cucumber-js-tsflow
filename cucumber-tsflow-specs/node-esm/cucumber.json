{
	"tsnodeesm": {
		"paths": ["../features/**/*.feature"],
		"import": ["./src/step_definitions/**/*.ts"],
		"loader": ["ts-node/esm"],
		"format": ["behave:../reports/tsnodeesm.json", "html:../reports/tsnodeesm.html", "junitbamboo:../reports/tsnodeesm.xml"],
		"parallel": 1,
		"tags": "@node-esm"
	},
		"esnodeesm": {
		"transpiler": "esnodeesm",
		"paths": ["../features/**/*.feature"],
		"import": ["./src/step_definitions/**/*.ts"],
		"format": ["behave:../reports/esnode.json", "html:../reports/esnode.html", "junitbamboo:../reports/esnode.xml"],
		"parallel": 0,
		"tags": "@node"
	}
}
