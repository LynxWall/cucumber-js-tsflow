{
	"esnode": {
		"transpiler": "esnode",
		"paths": ["../features/**/*.feature"],
		"require": ["./src/step_definitions/**/*.ts"],
		"format": ["behave:../cucumber_report_esnode.json", "html:../cucumber_report_esnode.html"],
		"parallel": 2,
		"tags": "@node",
		"publishQuiet": true
	},
	"tsnode": {
		"transpiler": "tsnode",
		"paths": ["../features/**/*.feature"],
		"require": ["./src/step_definitions/**/*.ts"],
		"format": ["behave:../cucumber_report_tsnode.json", "html:../cucumber_report_tsnode.html"],
		"parallel": 2,
		"tags": "@node",
		"publishQuiet": true
	}
}
